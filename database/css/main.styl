        width: 40px;
        height: 40px;
        position relative
    }
    .navbar-light .navbar-toggler span {
        width: 24px
        position: absolute;
        left 50%
        margin-left -12px
        height: 2px;
        top: 50%;
        overflow: hidden;
        text-indent: 200%;
        background-color #ccc
    }
    .navbar-light .navbar-toggler:before, .navbar-light .navbar-toggler:after {
        width: 24px
        left 50%
        margin-left -12px
        display: block;
        content: "";
        position: absolute;
        background-color #ccc
        top: 0;
        height: 2px;
        top: 50%;
        -webkit-transform-origin: 50% 50%;
        -ms-transform-origin: 50% 50%;
        transform-origin: 50% 50%;
    }
    .navbar-light .navbar-toggler:after {
        top 30%
    }
    .navbar-light .navbar-toggler:before {
        top auto
        bottom 30%
    }
    .navbar-light .navbar-toggler.active:before {
        top: 18px;
    }
    .navbar-light .navbar-toggler.active:after {
        bottom: 18px
        top auto
    }
    .navbar-light .navbar-toggler.active:before {
        -webkit-transform: rotate3d(0,0,1,45deg);
        transform: rotate3d(0,0,1,45deg);
    }
    .navbar-light .navbar-toggler.active span {
        opacity: 0;
    }
    .navbar-light .navbar-toggler.active:after {
        -webkit-transform: rotate3d(0,0,1,-45deg);
        transform: rotate3d(0,0,1,-45deg);
    }
}

